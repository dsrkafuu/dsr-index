    "chalk": "~5.0.0",
    "cross-env": "~7.0.3",
    "cssnano": "~5.0.14",
    "env-cmd": "~10.1.0",
    "html-minifier-terser": "~6.0.2",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.4.5",
    "prettier": "~2.5.1"
  }
}
